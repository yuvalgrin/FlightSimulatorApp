<Application x:Class="FlightSimulatorGui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FlightSimulatorGui"
             DispatcherUnhandledException="Application_DispatcherUnhandledException"
            xmlns:wpf="clr-namespace:Microsoft.Maps.MapControl.WPF;assembly=Microsoft.Maps.MapControl.WPF"
             xmlns:vm="clr-namespace:FlightSimulatorGui.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vm:MapViewModel x:Key="MapViewModel" />
        <vm:ControlRoomViewModel x:Key="ControlRoomViewModel" />
        <vm:JoystickViewModel x:Key="JoystickViewModel" />
        <vm:DataViewViewModel x:Key="DataViewViewModel" />
    </Application.Resources>
</Application>
